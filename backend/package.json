{
  "name": "moneywise-backend",
  "version": "1.0.0",
  "description": "Backend API pour l'application MoneyWise - Gestion des finances personnelles",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:api": "node test-api.js",
    "test:categories": "node test-categories.js",
    "test:transactions": "node test-transactions.js",
    "test:verification-email": "node test-verification-email.js",
    "diagnostic:email": "node diagnostic-email.js",
    "db:update-locale": "node mise-a-jour-db-locale.js",
    "db:check-locale": "node verifier-db-locale.js",
    "test:email-direct": "node test-email-direct.js",
    "test:forgot-password": "node test-forgot-password.js",
    "test:api-forgot-password": "node test-api-forgot-password.js",
    "db:init": "node init-db-local.js",
    "db:add-tokens": "node ajouter-table-tokens.js",
    "db:add-tokens-render": "node ajouter-table-tokens-render.js",
    "db:verifier-tables-render": "node verifier-tables-render.js",
    "db:init-render": "node init-db-render.js",
    "db:create": "createdb -U postgres moneywise",
    "db:seed": "psql -U postgres -d moneywise -f src/database/seed.sql",
    "db:reset": "dropdb -U postgres moneywise && npm run db:create && npm run db:init && npm run db:seed",
    "test:cors": "node test-cors-vercel.js",
    "test:url-config": "node test-url-config.js",
    "test:dev-urls": "node test-dev-urls.js"
  },
  "keywords": [
    "finance",
    "budget",
    "expenses",
    "income",
    "api",
    "nodejs",
    "express"
  ],
  "author": "MoneyWise Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
